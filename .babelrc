{
  "presets": [
    ["@babel/preset-env", {
      "corejs": 3
    }],
    "@babel/preset-react",
    "@babel/preset-flow"
  ],
  "plugins": [
    "react-loadable/babel",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-async-to-generator",
    "transform-flow-comments"
  ]
}
